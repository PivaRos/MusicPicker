{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  },

 "include": [
      "src/**/*.ts",
      "./DefaultAppConfig.json",
     "./src/rawHTML/*"    
    ],
  
}
